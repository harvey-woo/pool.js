{
  "name": "@cat5th/pool.js",
  "version": "0.1.0",
  "description": "Represents a generic object pool for javascript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
